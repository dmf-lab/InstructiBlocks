// Brick Class
class brick {
  String rfid;
  String type;
  String info;
  String name1;
  String rule1;
  String name2;
  String rule2;
  String name3;
  String rule3;
  String rule4;
  
  // Brick constructor
  brick(String rfid_, String type_, String info_, String name1_, String rule1_, String name2_, String rule2_, String name3_, String rule3_, String rule4_) {
    rfid = rfid_;
    type = type_;
    info = info_;
    name1 = name1_;
    rule1 = rule1_;
    name2 = name2_;
    rule2 = rule2_;
    name3 = name3_;
    rule3 = rule3_;
    rule4 = rule4_;
    
  }
}